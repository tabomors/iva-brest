{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf .cache public",
    "build": "npm run clean && gatsby build",
    "deploy:gh-pages": "npm run clean && gatsby build --prefix-paths && gh-pages -d public",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "lint": "eslint ."
  },
  "dependencies": {
    "dotenv": "^8.0.0",
    "gatsby": "^2.13.45",
    "gatsby-image": "^2.2.8",
    "gatsby-plugin-sharp": "^2.2.9",
    "gatsby-plugin-typography": "^2.3.2",
    "gatsby-source-contentful": "^2.1.17",
    "gatsby-source-filesystem": "^2.1.8",
    "gatsby-transformer-sharp": "^2.2.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-bootstrap": "^0.16.19"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.6.3",
    "eslint": "^6.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "gh-pages": "^2.0.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "rimraf": "^2.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
